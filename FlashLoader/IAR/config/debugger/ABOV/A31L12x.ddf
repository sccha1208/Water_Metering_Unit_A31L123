
;; Architecture extensions
#include weak "..\ARM\ARMv6-M-SYST.ddf"

;; Memory information ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; 
;; Used to define address zones within the ARM address space (Memory). 
;;
;;   Name      may be almost anything
;;   AdrSpace  must be Memory
;;   StartAdr  start of memory block
;;   EndAdr    end of memory block
;;   AccType   type of access, read-only (R), read-write (RW) or SFR (W)

[Memory]
;;         Name             AdrSpace    StartAdr    EndAdr      AccType   Width
Memory =  PPB              Memory      0xE0000000  0xE00FFFFF  W  
Memory =  DUMMY_SFR        Memory      0x50000000  0x5FFFFFFF  W  
Memory =  APB_SFR          Memory      0x40000000  0x4FFFFFFF  W  
Memory =  AHB_SFR          Memory      0x30000000  0x3FFFFFFF  W  
Memory =  SRAM             Memory      0x20000000  0x2FFFFFFF  RW 
Memory =  OPTION           Memory      0x1FFFF000  0x1FFFFFFF  R  
Memory =  BOOT             Memory      0x18000000  0x1FFFEFFF  R  
Memory =  FLASH            Memory      0x10000000  0x17FFFFFF  R  
Memory =  REMAP            Memory      0x00000000  0x0FFFFFFF  R  

TrustedRanges = true
UseSfrFilter = true

[SfrInclude]
File = A31L12x.svd

